{
  "name": "@draws/engine",
  "version": "1.1.2",
  "description": "Manage draws",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .ts --max-warnings=0 src",
    "test": "echo \"Error: no test specified\"",
